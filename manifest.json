{
    "manifest_version": 2,
    
    "name": "Hello_World",
    "description": "Print Hello, World! message",
    "version": "1.0",
    "content_scripts" : [
        {
            "matches" : ["https://www.facebook.com/*"],
            "js" : [
                "myscript.js"
            ],
            "run_at" : "document_end"
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "activeTab",
        "tabs", "https://www.facebook.com/*"
    ]
}